{
	"name": "svelte-next-tailwind",
	"version": "0.0.1",
	"scripts": {
		"validate": "svelte-check && tsc --noEmit",
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"lint:css": "stylelint **/*.{css,svelte}",
		"start": "svelte-kit start",
		"preformat": "rustywind --write ./src/",
		"format": "prettier --write '**/*.{svelte,js,html,md}' .github/**/*.yml --ignore-path .gitignore"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@tsconfig/svelte": "^1.0.10",
		"cssnano": "^4.1.10",
		"postcss": "^8.2.2",
		"postcss-cli": "^8.3.1",
		"postcss-load-config": "^3.0.0",
		"prettier": "^2.2.1",
		"prettier-plugin-svelte": "^1.4.2",
		"rustywind": "^0.7.0",
		"sirv-cli": "^1.0.10",
		"stylelint": "^13.8.0",
		"stylelint-config-standard": "^20.0.0",
		"svelte": "^3.38.2",
		"svelte-check": "^1.1.24",
		"svelte-preprocess": "^4.6.1",
		"tailwindcss": "^2.0.2",
		"tslib": "^2.0.3",
		"typescript": "^4.1.3",
		"vite": "^2.2.3"
	},
	"type": "module"
}
